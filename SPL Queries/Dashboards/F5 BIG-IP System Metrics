      <title>Control Plane CPU(%) per Core</title>
      <chart>
        <search>
          <query>index=netops Entity="CpuStat" | eval CPU_Usage_per_CPU_ID = system/100 | timechart span=1h max(CPU_Usage_per_CPU_ID) by cpu_id</query>
          <earliest>$time_token.earliest$</earliest>
          <latest>$time_token.latest$</latest>
