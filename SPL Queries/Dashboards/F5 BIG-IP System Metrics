      <title>Bits In/Out</title>
      <chart>
        <search>
          <query>index=netops system.tmmTraffic.serverSideTraffic.bitsOut=* | rename system.hostname as Host | timechart  max(system.tmmTraffic.clientSideTraffic.bitsIn) as clientBitsIn, max(system.tmmTraffic.clientSideTraffic.bitsOut) as clientBitsOut, max(system.tmmTraffic.serverSideTraffic.bitsIn) as serverBitsIn, max(system.tmmTraffic.serverSideTraffic.bitsOut) as serverBitsOut</query>
          <earliest>$time_token.earliest$</earliest>
          <latest>$time_token.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="charting.axisTitleX.visibility">collapsed</option>
