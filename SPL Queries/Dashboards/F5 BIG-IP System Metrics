      <chart>
        <search>
          <query>index=netops transactions&gt;= 0 (pool_name=* OR Entity='DeviceTraffic') |rename transactions as Transactions_Per_Second | timechart max(Transactions_Per_Second) by pool_name</query>
          <earliest>$time_token.earliest$</earliest>
          <latest>$time_token.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
