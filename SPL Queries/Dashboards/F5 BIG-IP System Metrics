    <panel>
      <title>Network Interface Metrics</title>
      <chart>
        <search>
          <query>index=netops (Entity="InterfaceTraffic" OR Entity="InterfaceHealth") | eval Dropped_Packets = counters_drops_in + counters_drops_out, Packet_Errors = counters_errors_in + counters_errors_out, Packets_In = counters_pkts_in, Packets_Out = counters_pkts_out | timechart span=1h max(Packets_In) as Packets_In, max(Packets_Out) as Packets_Out by hostname</query>
          <earliest>$time_token.earliest$</earliest>
          <latest>$time_token.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
